<%=header %>
                    <!-- 门店管理 -->
                    <div class="store">
                        <div class="panel panel-info">
                            <div class="panel-heading fz18">
                                门店信息
                                <button type="button" class="fr btn btn-danger add-store">添加</button>
                                <button type="button" class="fr btn btn-danger off-store on">取消</button>
                            </div>
                            <div class="panel-body">
                                <table class="table store-table table-bordered">
                                    <!-- 待请求的数据 -->
                                </table>
                                <form class="store-form form-horizontal on">
                                    <div class="form-group">
                                        <label for="inputStore" class="col-sm-2 control-label">店名：</label>
                                        <div class="col-sm-8">
                                            <input name="name" class="form-control" id="inputStore" placeholder="请输入门店名称">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="selet-oper" class="col-sm-2 control-label">绑定员工：</label>
                                        <div class="col-sm-8">
                                            <ul id="select-store-id" name="select-store-all">
                                            </ul>
                                        </div>
                                    </div>
                                    <div id="bind-em-roles" class="form-group clearfix">
                                        <label for="aaaa" class="col-sm-2 control-label"></label>
                                        <div class="col-sm-8">
                                            <ul class="clearfix"></ul>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="contactStore" class="col-sm-2 control-label">联系方式：</label>
                                        <div class="col-sm-8">
                                            <input name="contact" class="form-control" id="contactStore" placeholder="请输入联系方式">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-2 control-label">门店地址：</label>
                                        <!-- 门店地址定位 -->
                                        <div class="col-sm-2">
                                            <select name="input_province" id="input_province" class="form-control">
                                                <option value="">--请选择--</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-2">
                                            <select name="input_city" id="input_city" class="form-control">
                                                <option value="">--请选择--</option>
                                            </select>
                                        </div>
                                        <div class="col-sm-2">
                                            <select name="input_area" id="input_area" class="form-control">
                                                <option value="">--请选择--</option>
                                            </select>
                                        </div>
                                        <div style="margin-top:10px;" class="col-sm-8 col-sm-offset-2" >
                                            <input name="addr_detail" class="form-control" id="addr_detail" placeholder="详细街道">
                                            <div id="searchResultPanel" style="border:1px solid #C0C0C0;width:150px;height:auto; display:none;"></div>
                                        </div>
                                        <!-- <div id="map-warp" class="col-sm-8 col-sm-offset-2" style="margin-top:10px;">
                                        </div> -->
                                    </div>
                                    
                                    <div class="form-group">
                                        <div class="col-sm-offset-2 col-sm-8">
                                            <div class="btn btn-default store-btn-add">确定</div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
<%=footer %>